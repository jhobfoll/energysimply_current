body, html {
 height: auto;
}
.panel-full {
  position: relative;
  background-color: #fff;
  padding-bottom: 40px;
  padding-top: 30px;
}
.howwork{
  padding-top: 1px;
}
.panel-full.bg {
  background-color: #f7f6f6;
  margin: 0px;
}

.left-embed{
  width: 35%;
  margin: 21px auto;
}

.border-top {
  border-top: 1px solid #e7e6e6;
}
.icon-info{
  position: absolute;
  top: -36px;
  left: 50%;
  margin-left: -36.5px;
  width: 73px;
  height: 73px;
  border-radius: 50%;
}

.btn-brand {
  font-weight: bold;
  border: 1px solid #B30808;
  background-color: #e40a0a;
  background-image: -webkit-gradient(linear, left top, left bottom, from(#e40a0a), to(#9f0202));
  background-image: -webkit-linear-gradient(top, #e40a0a, #9f0202);
  background-image: linear-gradient(to bottom, #e40a0a, #9f0202);
  filter: progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr=#e40a0a,endColorstr=#9f0202);
}

.btn-brand:hover {
  background-color: #f62b2b;
  background-image: -webkit-gradient(linear, left top, left bottom, from(#f62b2b), to(#d20202));
  background-image: -webkit-linear-gradient(top, #f62b2b, #d20202);
  background-image: linear-gradient(to bottom, #f62b2b, #d20202);
  filter: progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr=#f62b2b,endColorstr=#d20202);
}

.page-signup-btn-how-it-works {
  padding: 30px 0 20px 0;
  text-align: center;
}

.page-signup-btn-how-it-works .btn {
  letter-spacing: .05em;
  border-radius: 25px;
  -webkit-border-radius: 25px;
  padding: 7px 30px;
  font-size: 15px;
  color: #fff;
}

.page-top-title {
  position: relative;
  text-align: center;
  border-bottom: 1px solid #c8c8c8;
  max-width: 500px;
  margin: 0 auto;
 }

.very-top{
  padding-top: 0px;
}

.lower-down {
  padding-top: 20px;
}

.page-top-title h1{
  color: #2d3e50;
}
.page-top-title.max-width-335 {
  max-width: 335px;
}

.page-top-title.max-width-450 {
  max-width: 450px;
}

@media (max-width: 450px) {
  .page-top-title.max-width-450 {
    max-width: auto;
  }

  .page-top-title.max-width-325 {
    max-width: auto;
  }

  .page-top-title {
    max-width: auto;
  }
}
.page-top-title.max-width-690 {
  max-width: 690px;
}

@media (max-width: 690px) {
  .page-top-title.max-width-690 {
    max-width: auto;
  }
}
.page-top-title.max-width-1000 {
  max-width: 1000px;
}

@media (max-width: 1000px) {
  .page-top-title.max-width-1000 {
    max-width: auto;
  }

  .page-top-title.max-width-1000 h2 br {
    display: none;
  }
}
.page-top-title::after {
  position: absolute;
  left: 0;
  bottom: -2px;
  content: "";
  width: 100%;
  border-bottom: 1px solid #fff;
}

.page-top-title h2 {
  font-size: 25px;
  color: #5d5b5b;
  text-shadow: 1px 1px 0 rgba(255, 255, 255, 0.4);
  padding-bottom: 3px;
  margin-top: 0;
  text-transform: uppercase;
  line-height: 1.3;
}

@media (max-width: 767px) {
  .page-top-title h2 {
    font-size: 20px;
  }

  .page-top-title h2 br {
    display: none;
  }
}
@media (max-width: 1000px) {
  .page-top-title.max-width-1000 {
    max-width: auto;
  }
}

/**
 * block 1
 */

.top-title-box-wrapper {
  width: 800px;
  margin: 0 auto;
  padding-top: 18px;
  padding-bottom: 12px;
}

.top-title-box {
  float: left;
  display: block;
  width: 134px;
  color: #646567;
  margin-right: 32px;
  text-align: center;
}

.top-title-box.last {
  margin-right: 0;
}

.top-title-box:hover,
.top-title-box:focus {
  text-decoration: none;
  color: #646567;
}

.top-title-box h3 {
  font-size: 17px;
  margin: 0  0 10px 0;
  height: 46px;
  line-height: 1.3;
}

.top-title-box .icon-box {
  height: 91px;
  border: 5px solid #faf9f9;
  background-color: #fff;
  border-radius: 3px;
  box-shadow: 0.5px 0.866px 3px 0px rgba(15, 16, 16, 0.4);
  text-align: center;
  padding-top: 3px;
}

@media (max-width: 840px) {
  .top-title-box-wrapper {
    width: auto;
  }

  .top-title-box.last,
  .top-title-box {
    float: none;
    display: block;
    margin: 0 auto;
  }

  .top-title-box h3 {
    padding-top: 13px;
    height: auto;
    padding-bottom: 5px;
  }
}

/**
 * block 2
 */

.sign-inner-wrap {
  width: 644px;
  margin: 0 auto;
  padding-top: 42px;
  padding-bottom: 12px;
}

.sign-row {
  height: 82px;
  margin-bottom: 32px;
}

.sign-row.last {
  margin-bottom: 0;
}

.text-left {
  position: relative;
  float: left;
  width: 355px;
  height: 82px;
}

.text-left::after {
  display: block;
  position: absolute;
  top: 50%;
  margin-top: -9px;
  right: 0;
  content: "";
  width: 15px;
  height: 18px;
}

.img-right {
  float: right;
  width: 268px;
}

.number-text {
  display: block;
  float: left;
  border-radius: 50%;
  background-color: #858789;
  width: 52px;
  height: 52px;
  color: #fff;
  line-height: 52px;
  text-align: center;
  font-size: 30px;
  font-weight: bold;
  margin-top: 14px;
}

.text-left h3 {
  float: left;
  font-size: 19px;
  font-weight: normal;
  margin: 30px 0 0 20px;
  color: #72767a;
}

@media (max-width: 650px) {
  .sign-inner-wrap {
    width: auto;
  }

  .sign-row {
    height: auto;
    width: 290px;
    margin: 0 auto;
    padding-bottom: 10px;
  }

  .text-left {
    text-align: left;
    float: none;
    width: 100%;
    height: 82px;
  }

  .number-text {
    display: inline-block;
    float: none;
    margin-top: 14px;
    width: 40px;
    height: 40px;
    line-height: 40px;
    font-size: 24px;
    margin-top: 14px;
  }

  .text-left h3 {
    float: none;
    display: inline-block;
    margin: 0;
    padding-left: 10px;
  }

  .img-right {
    float: none;
    width: auto;
    padding-top: 10px;
  }

  .img-right img {
    display: block;
    max-width: 100%;
    height: auto;
    margin: 0 auto;
  }

  .text-left::after {
    position: absolute;
    top: auto;
    margin-top: 0;
    right: auto;
    bottom: 0;
    left: 50%;
    margin-left: -7.5px;
    -ms-transform: rotate(90deg);
    -webkit-transform: rotate(90deg);
    transform: rotate(90deg);
  }
}

/**
 * block 3
 */

.we-use-inner-wrap {
  width: 925px;
  margin: 0 auto;
  min-height: 150px;
  padding-top: 32px;
  padding-bottom: 12px;
}

.plan-box {
  position: relative;
  float: left;
  margin-right: 96px;
}

.plan-box.plus-icon::after {
  display: block;
  position: absolute;
  top: 50%;
  margin-top: -18px;
  right: -75px;
  content: "";
  background-image:url(<%= asset_path('img/plus-large.png') %>);
  width: 57px;
  height: 69px;
}

.plan-box.equal-icon::after {
  display: block;
  position: absolute;
  top: 50%;
  margin-top: -1px;
  right: -75px;
  content: "";
  background-image:url(<%= asset_path('img/equal-large.png') %>);
  width: 48px;
  height: 39px;
}

.plan-box h3 {
  margin: 0 0 15px 0;
  font-size: 19px;
  color: #72767a;
}

.plan-box h3 span {
  display: inline-block;
  background-color: #72767a;
  width: 15px;
  height: 15px;
  margin-right: 8px;
}

.plan-box > .plan-img-box {
  border: 5px solid #faf9f9;
  background-color: #fff;
  box-shadow: 0.5px 0.866px 3px 0px rgba(15, 16, 16, 0.5);
  width: 234px;
  height: 225px;
}

.plan-box.last {
  margin-right: 0;
}

@media (max-width: 900px) {
  .we-use-inner-wrap {
    width: auto;
  }

  .plan-box.last,
  .plan-box {
    float: none;
    width: 240px;
    margin: 0 auto;
  }

  .plan-box.equal-icon {
    padding-top: 105px;
  }

  .plan-box.last {
    padding-top: 75px;
  }

  .plan-box.plus-icon::after {
    display: block;
    position: absolute;
    top: auto;
    margin-top: auto;
    right: auto;
    left: 50%;
    margin-left: -28px;
    bottom: -90px;
  }

  .plan-box.equal-icon::after {
    top: auto;
    margin-top: auto;
    right: auto;
    bottom: -60px;
    left: 50%;
    margin-left: -24px;
  }
}

/**
 * block 4
 */

.autopay-inner {
  width: 932px;
  margin: 0 auto;
  padding-top: 40px;
  padding-bottom: 5px;
}

.autopay-inner .image-left {
  float: left;
  margin-right: 30px;
}

.autopay-inner .image-right {
  float: right;
}

.autopay-inner figure img {
  display: block;
  max-width: 100%;
  height: auto;
  margin: 0 auto;
}

.autopay-inner figure figcaption {
  display: block;
  color: #72767a;
  font-size: 18px;
  text-align: center;
  padding-top: 22px;
}

@media (max-width: 932px) {
  .autopay-inner {
    width: auto;
  }

  .autopay-inner .image-right,
  .autopay-inner .image-left {
    float: none;
    margin-right: 0;
    padding: 10px;
  }

  .autopay-inner figure figcaption {
    padding: 20px 0;
  }
}

/**
 * block 5
 */

.your-plan-inner {
  width: 1015px;
  margin: 0 auto;
  padding-top: 50px;
}

.your-plan-title h3 {
  margin: 0 0 15px 0;
  font-size: 19px;
  color: #72767a;
}

.your-plan-title h3 span {
  display: inline-block;
  background-color: #72767a;
  width: 15px;
  height: 15px;
  margin-right: 8px;
}

.calendar-wrapper {
  position: relative;
  float: left;
  width: 360px;
}

.calendar-wrapper::after {
  display: block;
  position: absolute;
  content: "";
  top: 66px;
  right: -30px;
  background-image:url(<%= asset_path('img/arrow-wide.png') %>);
  width: 31px;
  height: 159px;
}

.calendar-box {
  position: relative;
  background-image:url(<%= asset_path('img/calender-img.png') %>);
  width: 219px;
  height: 232px;
  background-repeat: no-repeat;
}

.big-arrow {
  position: absolute;
  top: 103px;
  left: 78px;
  background-image:url(<%= asset_path('img/big-arrow.png') %>);
  width: 177px;
  height: 51px;
  -webkit-transform: rotate(0deg);
      -ms-transform: rotate(0deg);
          transform: rotate(0deg);
}

.calendar-box .arrow-text {
  color: #5d5b5b;
  font-size: 18px;
  position: absolute;
  top: 88px;
  right: -120px;
}

.proposal-wrapper {
  position: relative;
  float: left;
  margin-left: 51px;
}

.proposal-wrapper::after {
  display: block;
  position: absolute;
  content: "";
  top: 66px;
  right: -62px;
  background-image:url(<%= asset_path('img/arrow-wide.png') %>);
  width: 31px;
  height: 159px;
}

.proposal-exam {
  width: 231px;
  height: 159px;
  background-color: #fff;
  box-shadow: 0.5px 0.866px 3px 0px rgba(15, 16, 16, 0.5);
  margin-top: 43px;
  padding: 12px 0 0 17px;
  color: #72767a;
  font-size: 16px;
  line-height: 1.2;
}

.automatic-wrapper {
  float: left;
  width: 280px;
  margin-left: 92px;
}

.automatic-img img {
  display: block;
  max-width: 100%;
  height: auto;
  margin: 0 auto;
}

@media (max-width: 1015px) {
  .your-plan-inner {
    width: auto;
  }

  .page-top-title.max-width-1000 {
    max-width: 767px;
  }

  .page-top-title.max-width-1000 h2 br {
    display: none;
  }

  .calendar-wrapper {
    float: none;
    width: 219px;
    margin: 0 auto;
  }


  .big-arrow {
    top: 208px;
    left: -4px;
    -ms-transform: rotate(98deg);
    -webkit-transform: rotate(98deg);
    transform: rotate(98deg);
  }

  .calendar-box .arrow-text {
    top: 328px;
    right: auto;
    left: 70px;
  }

  .calendar-wrapper::after {
    top: auto;
    right: auto;
    bottom: -110px;
    left: 50%;
    margin-left: -15px;
    -ms-transform: rotate(90deg);
    -webkit-transform: rotate(90deg);
    transform: rotate(90deg);
  }

  .calendar-box {
    height: 364px;
  }

  .proposal-wrapper {
    float: none;
    margin: 0 auto;
    width: 219px;
    padding-top: 60px;
  }

  .proposal-wrapper::after {
    top: auto;
    right: auto;
    bottom: -110px;
    left: 50%;
    margin-left: -15px;
    -ms-transform: rotate(90deg);
    -webkit-transform: rotate(90deg);
    transform: rotate(90deg);
  }

  .proposal-exam {
    margin-top: 0;
    width: 219px;
    padding: 15px 0 0 10px;
  }

  .automatic-wrapper {
    float: none;
    width: 219px;
    margin: 0 auto;
    padding-top: 65px;
  }
}

/**
 * block 6
 */

.tracking-inner {
  width: 1106px;
  margin: 0 auto;
  padding-top: 40px;
  padding-bottom: 12px;
}

.tracking-title {
  margin-left: 1px;
}

.tracking-title h3 {
  margin: 0 0 15px 0;
  font-size: 16px;
  color: #72767a;
}

.tracking-title h3 span {
  display: inline-block;
  background-color: #72767a;
  width: 15px;
  height: 15px;
  margin-right: 4px;
}

.tracking-box {
  position: relative;
  float: left;
  width: 250px;
  margin-right: 43px;
}

.tracking-box.last {
  margin-right: 0;
  width: 226px;
}

.tracking-box.arrow::after {
  display: block;
  position: absolute;
  content: "";
  top: 55px;
  right: -20px;
  background-image:url(<%= asset_path('img/arrow-wide2.png') %>);
  width: 21px;
  height: 98px;
}

@media (max-width: 1106px) {
  .tracking-inner {
    width: auto;
    padding: 40px 10px 12px 10px;
  }

  .tracking-box.arrow::after {
    top: auto;
    right: auto;
    left: 50%;
    margin-left: -17.5px;
    bottom: -45px;
    -ms-transform: rotate(90deg);
    -webkit-transform: rotate(90deg);
    transform: rotate(90deg);
  }
  .tracking-title h3 {
    margin: 15px 0 15px 0;
  }

  .tracking-box {
    margin-right: 10px;
    width: 240px;
  }
}
@media (max-width: 1015px) {
  .tracking-inner {
    width: auto;
  }

  .tracking-box.last,
  .tracking-box {
    width: 240px;
    margin: 0 auto;
    float: none;
    padding-bottom: 20px;
  }

  .tracking-box.last {
    padding-bottom: 0;
  }
}

@media (max-width: 840px) {
  .left-embed{
    width: 75%;
    margin: 21px auto;
  }
}